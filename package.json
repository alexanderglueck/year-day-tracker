        "resolve-url-loader": "^3.1.1",
        "sass": "^1.26.2",
        "sass-loader": "^7.3.1",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "@sweetalert2/theme-bootstrap-4": "^3.1.1",
        "fullcalendar": "^3.10.1",
        "invert-color": "^2.0.0",
        "moment": "^2.24.0",
        "sweetalert2": "^9.8.2"
    }
